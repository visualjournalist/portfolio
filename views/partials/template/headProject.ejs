	<title><%- data[0].title %> <%if (pageTitle!=''){%> :: <%= pageTitle %> <% } %></title>
	<meta name='viewport' content='width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0'/>
	<meta http-equiv="content-type" content="text/html;charset=utf-8" />

	<link rel="apple-touch-icon-precomposed" sizes="120x120" href="images/retina_icon.png">

	<!-- for Google -->
	<meta name="description" content="<%- metaDescription %>"/>
	<meta name="keywords" content="<%= data[0].metakeywords %>" />
	<meta name="author" content="<%= data[0].name %>" />

	<!-- for Facebook -->
	<meta property="og:title" content="<%- data[0].title %>  :: <%= pageTitle %>" />
	<meta property="og:type" content="<%- data[0].metatype %>" />
	<meta property="og:image" content="<%= metaImage %>" />
	<meta property="og:url" content="<%- url %>" />
	<meta property="og:description" content="<%- metaDescription %>" />

	<link href='http://fonts.googleapis.com/css?family=Oswald' rel='stylesheet' type='text/css'>
	<link rel='stylesheet' href='/stylesheets/styles.css' />