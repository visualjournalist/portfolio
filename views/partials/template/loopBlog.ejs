			<% for (var i=blogData.length-1;i>=0; i--){ %>

			<div class='entry' data-blog-number=<%= i %> >
				<div class='floatLeft category <%= blogData[i].category %>'>
				<h3 class='category'><a href='/portfolio/<%= blogData[i].category %>'><%= blogData[i].category %></a></h3>
				</div>
				<div class='floatRight'><h3 class='date'><%= blogData[i].date %> %></h3></div>
				<div class='clearMe'></div>
				<h2 class='headline'><%= blogData[i].title %></h2>



					<a href='<%= blogData[i].projectlink %>' target='_blank'><img src='<%= blogData[i].image %>' class='<% if(blogData[i].verticalimage=='TRUE') {%>verticalImage<% } %>'/></a>


				<div class='description'><%- blogData[i].postSplit %></div>



			</div>

			<% } %>
